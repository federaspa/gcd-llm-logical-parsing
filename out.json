{
  "facts": [
    "is(John,Tall)",
    "is(Henry,Young)",
    "is(Dave,Quiet)",
    "is(John,Tall)",
    "is(Dave,Strong)",
    "is(John,Strong)",
    "is(Bob,Big)"
  ],
  "rules": [
    "is(?X,Young) ∧ is(?X,Quiet) → is(?X,Big).",
    "is(?X,Big) → is(?X,Round).",
    "is(?X,Tall) → is(?X,Smart).",
    "is(?X,Strong) ∧ is(?X,Quiet) → is(?X,Round).",
    "is(?X,Tall) → is(?X,Quiet).",
    "is(?X,Young) ∧ is(?X,Tall) → is(?X,Big).",
    "is(Dave,Big) → is(Dave,Young).",
    "is(?X,Strong) → is(?X,Quiet)."
  ]
}