root ::= FOLJSON

FOLJSON ::= "{" ws folpreds "," ws folconsts "," ws folrules "," ws folconc "}"

folpreds ::= "\"folpreds\": [" [Predicate ("," Predicate)*] "]"
folconsts ::= "\"folconsts\": [" [Constant ("," Constant)*] "]"
folrules ::= "\"folrules\": [" [FOLRule ("," FOLRule)*] "]"
folconc ::= "\"folconc\": " FOLRule

Predicate ::= "\"" [A-Za-z]+ "(" [A-Za-z, ]+ ")" "\""
Constant ::= "\"" [a-z0-9]+ ("_" [a-z0-9]+)* "\""

FOLRule ::= "\"" Quantifier? "(" [A-Za-z]+ ((","|" ") Variable)* ")" (LogicalConnective FOLRule)? "\""
           | "\"" ¬?[A-Za-z]+ "(" [A-Za-z0-9_, ]+ ")" (LogicalConnective FOLRule)? "\""
           | "\"" [A-Za-z]+ "(" [A-Za-z0-9_, ]+ ")" "\""

Quantifier ::= "∀" [A-Za-z] | "∃" [A-Za-z]
Variable ::= [A-Za-z]
LogicalConnective ::= "→" | "∧" | "∨" | "↔" | "⊕" | "¬"

ws ::= [ \t\n]*