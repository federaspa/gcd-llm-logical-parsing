root ::= "{" ws preds ws consts ws rules ws conc ws "}"

preds ::= "\"fol_preds\"" ws ":" ws array ws "," ws

consts ::= "\"fol_consts\"" ws ":" ws array ws "," ws

rules ::= "\"fol_rules\"" ws ":" ws array ws "," ws

conc ::= "\"fol_conc\"" ws ":" ws string

array ::= "[" ws (string ("," ws string)*)? "]" ws

string ::=
  "\"" (
    [^"\\\x7F\x00-\x1F] |
    "\\" (["\\bfnrt] | "u" [0-9a-fA-F]{4}) # escapes
  )* "\"" ws

# Optional space: by convention, applied in this grammar after literal chars when allowed
ws ::= | " " | "\n" [ \t]{0,5}