root ::= "{" ws preds ws consts ws "}"

ws ::= [ \t\n\r]*

preds ::= "\"fol_preds\"" ws ":" ws predarray ws "," ws

consts ::= "\"fol_consts\"" ws ":" ws constarray ws "," ws

predarray ::= "[" ws (pred ("," ws pred)*)? "]" ws
constarray ::= "[" ws (const ("," ws const)*)? "]" ws

pred ::= "\"" [A-Z0-9][a-zA-Z0-9]* ws "(" ws term ("," ws term)* ")" "\"" ws

term = const | variable

const ::= [a-z]+
variable = [a-z]

# Optional space: by convention, applied in this grammar after literal chars when allowed
ws ::= | " " | "\n" [ \t]{0,5}