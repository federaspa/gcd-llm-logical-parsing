root ::= S

S ::= "¬"? (QUANT VAR ws){0,2} "(" B (ws OP ws B){0,4} ")"
B ::= "¬"? (QUANT VAR ws){0,2} "(" C (ws OP ws C){0,4} ")"
C ::= "¬"? (QUANT VAR ws){0,2} "(" D (ws OP ws D){0,4} ")"
D ::= "¬"? (QUANT VAR ws){0,2} "(" E (ws OP ws E){0,4} ")"
E ::= "¬"? (QUANT VAR ws){0,2} "(" F (ws OP ws F){0,4} ")"
F ::= "¬"? (QUANT VAR ws){0,2} "(" G (ws OP ws G){0,4} ")"

G ::= "¬"? PRED "(" TERMS ")"
TERMS ::= TERM ("," ws TERM)*
TERM ::= VAR | CONST

OP ::= "\u2295" | "\u2228" | "\u2227" | "\u2192" | "\u2194"

QUANT ::= "\u2200" | "\u2203"

CONST ::= [a-z0-9]{2,15}

VAR ::= "x" | "y" | "z"

PRED ::= [[PREDICATES]]

ws ::= " "