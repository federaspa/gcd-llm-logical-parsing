root ::= "Valid FOL Statement:\n\n"formula"\n------"

formula ::= (quantifier term)* expression (logoperator expression)*

expression ::= "(" ("¬"? statement logoperator ws)* "¬"? statement ")"

statement ::= predicatename "(" terms ")" 

terms ::= term ("," term)*

term ::= variable | constant

variable ::=  "w" | "x" | "y" | "z"
constant ::= [a-z]+

quantifier ::= "\u2200" | "\u2203"
logoperator ::= "\u2227" | "\u2228" | "\u2295" | "\u2192" | "\u2194"

predicatename ::= "Square" | "Shape" | "FourSides"