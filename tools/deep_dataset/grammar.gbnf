theory ::= statement+
statement ::= fact | rule
fact ::= ( entity ws "is" ws attribute ws polarity )
| ( entity ws relation ws entity ws polarity )

rule ::= ( fact (ws fact)* ws "â†’" ws fact )

entity ::= name | variable

name ::= [[NAMES]]
variable ::= "something" | "someone" | "thing"
attribute ::= [[ATTRIBUTES]]
relation ::= [[RELATIONS]]
polarity ::= "+" | "-"